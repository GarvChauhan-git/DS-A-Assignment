int Maxplatform(int arr[[, int dep[] ,int n)
{
  sort(arr, arr+n);
  sort(dep, dep+n);
  int i=1,int j=0;
  int currplatform=1,maxplatform=1;
  while(i<n && j<n)
  {
    if(arr[i]<dep[j]){
       i++;
       currplatform +=1;
       if(max<currplatform){
          max = currentplatform;
       }
    }
    else if(arr[i]>dep[j])
    {
       j++;
       currplatform -=1;
    }
  }
  return max;
  
}
